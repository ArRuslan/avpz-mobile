<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:UniMobileProject.src.Views"
             x:Class="UniMobileProject.src.Views.MainTabbedPage"
             xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
             android:TabbedPage.ToolbarPlacement="Bottom"
             Title="MainTabbedPage" 
             NavigationPage.HasNavigationBar="False" 
             BarBackgroundColor ="{DynamicResource PrimaryColor}" 
             BackgroundColor="{DynamicResource PageBackgroundColor}" 
             SelectedTabColor="#FFFFFF" 
             UnselectedTabColor ="#000000">

    <local:MainPage Title="Main" IconImageSource="main_page_icon.png" />
    <local:UserBookingsPage Title="Bookings" IconImageSource="user_bookings_page_icon.png" />
    <local:UserProfilePage Title="My Profile" IconImageSource="user_profile_page_icon.png" />

</TabbedPage>